Q1
#include<stdio.h>
void main()
{
int n;
printf("Enter number:")
scanf("%d",&n);
if(n%2==0)
{
printf("the number is even");
}
else
{
printf("Number is odd");
}
return 0;
} 

Q2
#include<stdio.h>
void main()
{
int x,y,z;
printf("Enter first number:";
scanf("%d",&x);
printf("Enter second number:";
scanf("%d",&y);
printf("Enter third number:";
scanf("%d",&z);
if (x>y && y>z)
{
printf("The largest number is%d",x);
}
else if (y>x&&y>z)
{
printf("The largest number is%d",y);
}
else 
{
printf("The largest number is%d",z;
}
return 0;
}

Q3
#include<stdio.h>
void main()
{
int a,b,c,d;
float root1,root2,re,im,disc;
printf("Enter a:");
scanf("%d",&a);
printf("Enter b:");
scanf("%d",&b);
printf("Enter c:");
scanf("%d",&c);
disc=b*b-4*a*c;
if(disc<0)
{
d=0;
}
else if(disc<0)
{
d=1
}
switch(d) 
{
case0:
re=-b/2*a
im=sqrt(fabs(disc))/2*a
printf("Imaginary roots\n Root1=%.2f+%.2f,re,im);
printf("Root2=%.2f-%.2fi",re,im);
break
Case 1:
printf("Roots are real\n root=%.2f",-b/2*a;
default:
root1=(-b+sqrt(disc))/2*a);
root1=(-b-sqrt(disc))/2*a);
printf("Roots are\n %f and %f", root1,root2);
}
return 0;
}

Q4
#include<stdio.h>
void main()
{
int x,y,z;
printf("Enter first number=")
scanf("%d",x);
printf("Enter second number=")
scanf("%d",y);
printf("Enter third number=")
scanf("%d",z);
if(x<y&&x<z)
{
printf("The smallest number%d",x);
}
else if(y<z&&y<z)
{
printf("The smallest number%d",y);
}
else 
{
printf("The smallest number%d",z);
}
}

Q5
#include<stdio.h>
void main()
{
int num;
printf("Enter a number:);
scanf("%d",&num);
if (num>0)
{
printf("The number is positive");
}
else if (num<0)
{
printf("The number is negative");
}
else
{
printf("The number is zero");
}
return 0;
}

Q6
#include<stdio.h>
void main()
{
int num;
printf("Enter number of days:");
scanf("%d",&num);
if(num<=5)
{
printf("Fine is Rs 0.50");
}
else if (num<=10)
{
printf("Fine is Rs 1.00");
}
else
{
printf("Fine is Rs 10.00);
}
return 0;
}

Q7
#include <stdio.h>
int main() 
{
  int x, y;
  printf("Enter the value of x: ");
  scanf("%d", &x);
  if (x > 0) 
{
  y = 1;
} 
    else if (x == 0) 
{
   y = 0;
} 
else 
{
   y = -1;
}
printf("Y = %d\n", y);
return 0;
}
